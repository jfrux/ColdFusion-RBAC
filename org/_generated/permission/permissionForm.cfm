<form action="<cfoutput>#xfa.frmSubmit#</cfoutput>" method="post" class="" ><input type="hidden" name="permissionID" value="<cfoutput>#permissionID#</cfoutput>" /><div class="req"><b>*</b> Indicates required field</div><cfoutput><h3>Form title here</h3><fieldset><legend>Section Legend<legend><div class="notes">	<h4>permission Info</h4></div>	<div>	<label for="name"><em>*</em>name:</label>	<input id="name" name="name" type="text" class="inputText" value="#permissionBean.getname()#"  />	</div>	<div>	<label for="description">description:</label>	<input id="description" name="description" type="text" class="inputText" value="#permissionBean.getdescription()#"  />	</div>	<div>	<label for="object_id"><em>*</em>object_id:</label>	<input id="object_id" name="object_id" type="text" class="inputText" value="#permissionBean.getobject_id()#"  />	</div>	<div>	<label for="operation_id"><em>*</em>operation_id:</label>	<input id="operation_id" name="operation_id" type="text" class="inputText" value="#permissionBean.getoperation_id()#"  />	</div>	<br />	<div class="">		<input type="submit" value="Submit" class="" /><br />	</div></fieldset></cfoutput></form>
