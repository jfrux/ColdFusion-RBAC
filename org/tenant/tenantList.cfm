<h1>tenant List</h1><p>This is a list of all tenants that have been created.</p><table border="1" cellpadding="2" cellspacing="0">	<thead>		<tr>					<th>id</th>						<th>name</th>						<th>tenantkind_id</th>						<th>address1</th>						<th>address2</th>						<th>address3</th>						<th>city</th>						<th>state</th>						<th>postalcode</th>						<th>country</th>						<th>phone1</th>						<th>phone2</th>						<th>phone3</th>						<th>fax1</th>						<th>fax2</th>						<th>uuid</th>						<th>created</th>						<th>createdby</th>						<th>updated</th>						<th>updatedby</th>						<th>deleted</th>						<th>deletedflag</th>						<th>deletedby</th>						<th>Actions</th>		</tr>	</thead>	<tbody>		<cfoutput query="qtenants">			<cfif currentRow Mod 2>				<cfset class="odd">			<cfelse>				<cfset class="even">			</cfif>						<tr class="#class#">							<td>#id#</td>							<td>#name#</td>							<td>#tenantkind_id#</td>							<td>#address1#</td>							<td>#address2#</td>							<td>#address3#</td>							<td>#city#</td>							<td>#state#</td>							<td>#postalcode#</td>							<td>#country#</td>							<td>#phone1#</td>							<td>#phone2#</td>							<td>#phone3#</td>							<td>#fax1#</td>							<td>#fax2#</td>							<td>#uuid#</td>							<td>#created#</td>							<td>#createdby#</td>							<td>#updated#</td>							<td>#updatedby#</td>							<td>#deleted#</td>							<td>#deletedflag#</td>							<td>#deletedby#</td>							<td><a href="#myself#ctenant.edittenant&tenantID=#tenantid#">Edit </a> | <a href="#myself#ctenant.deletetenant&tenantID=#tenantid#">Delete</a></td>			</tr>		</cfoutput>	</tbody></table><cfoutput>[<a href="#myself#ctenant.newtenant">Add New tenant</a>]</cfoutput>
