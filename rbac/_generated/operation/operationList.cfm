<h1>operation List</h1><p>This is a list of all operations that have been created.</p><table border="1" cellpadding="2" cellspacing="0">	<thead>		<tr>					<th>id</th>						<th>name</th>						<th>description</th>						<th>_create</th>						<th>_read</th>						<th>_update</th>						<th>_delete</th>						<th>locked</th>						<th>Actions</th>		</tr>	</thead>	<tbody>		<cfoutput query="qoperations">			<cfif currentRow Mod 2>				<cfset class="odd">			<cfelse>				<cfset class="even">			</cfif>						<tr class="#class#">							<td>#id#</td>							<td>#name#</td>							<td>#description#</td>							<td>#_create#</td>							<td>#_read#</td>							<td>#_update#</td>							<td>#_delete#</td>							<td>#locked#</td>							<td><a href="#myself#coperation.editoperation&operationID=#operationid#">Edit </a> | <a href="#myself#coperation.deleteoperation&operationID=#operationid#">Delete</a></td>			</tr>		</cfoutput>	</tbody></table><cfoutput>[<a href="#myself#coperation.newoperation">Add New operation</a>]</cfoutput>
