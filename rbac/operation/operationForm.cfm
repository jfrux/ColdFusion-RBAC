<form action="<cfoutput>#xfa.frmSubmit#</cfoutput>" method="post" class="" ><input type="hidden" name="operationID" value="<cfoutput>#operationID#</cfoutput>" /><div class="req"><b>*</b> Indicates required field</div><cfoutput><h3>Form title here</h3><fieldset><legend>Section Legend<legend><div class="notes">	<h4>operation Info</h4></div>	<div>	<label for="name"><em>*</em>name:</label>	<input id="name" name="name" type="text" class="inputText" value="#operationBean.getname()#"  />	</div>	<div>	<label for="description">description:</label>	<input id="description" name="description" type="text" class="inputText" value="#operationBean.getdescription()#"  />	</div>	<div>	<label for="_create">_create:</label>	<input id="_create" name="_create" type="text" class="inputText" value="#operationBean.get_create()#"  />	</div>	<div>	<label for="_read">_read:</label>	<input id="_read" name="_read" type="text" class="inputText" value="#operationBean.get_read()#"  />	</div>	<div>	<label for="_update">_update:</label>	<input id="_update" name="_update" type="text" class="inputText" value="#operationBean.get_update()#"  />	</div>	<div>	<label for="_delete">_delete:</label>	<input id="_delete" name="_delete" type="text" class="inputText" value="#operationBean.get_delete()#"  />	</div>	<div>	<label for="locked">locked:</label>	<input id="locked" name="locked" type="text" class="inputText" value="#operationBean.getlocked()#"  />	</div>	<br />	<div class="">		<input type="submit" value="Submit" class="" /><br />	</div></fieldset></cfoutput></form>
